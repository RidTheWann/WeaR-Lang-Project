// Test: Advanced Control Flow (if/else-if/else)
var nilai = 75

jika (nilai >= 90) {
    cetak "Grade: A"
} tapi_jika (nilai >= 80) {
    cetak "Grade: B"
} tapi_jika (nilai >= 70) {
    cetak "Grade: C"
} tapi_jika (nilai >= 60) {
    cetak "Grade: D"
} lainnya {
    cetak "Grade: F"
}

cetak "Selesai!"
